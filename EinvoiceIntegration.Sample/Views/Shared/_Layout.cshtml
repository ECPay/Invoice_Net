                    <li>@Html.ActionLink("上傳開立發票(離線)", "Index", "B2CInvoiceOfflineIssue")</li>
                    <li>@Html.ActionLink("上傳作廢發票(離線)", "Index", "B2CInvoiceOfflineInvalid")</li>
